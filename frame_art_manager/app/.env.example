# Environment Variables Template
# Copy this file to .env and update with your local paths

# Path to your frame art directory
# For development, point this to your Home Assistant www/frame_art directory
# Example: /home/username/ha-config/www/frame_art
FRAME_ART_PATH=/path/to/your/ha-config/www/frame_art

# Server port (optional, defaults to 8099)
# PORT=8099
